/*搜索框css*/
.weui-search-bar {
  margin-top: 10rpx;
  position: relative;
  padding: 16rpx 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: white;
  border-top: 2rpx solid #ffffff;
  border-bottom: 2rpx solid #ffffff;
}
.weui-icon-search_in-box {
  position: absolute;
  left: 20rpx;
  top: 14rpx;
}
.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
  border-radius: 10rpx;
  background: #FFFFFF;
  border: 2rpx solid #E6E6EA;
}
.weui-search-bar__box {
  position: relative;
  padding-left: 60rpx;
  padding-right: 66rpx;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}
.weui-search-bar__input {
  height: 56rpx;
  line-height: 56rpx;
  font-size: 28rpx;
}
.weui-search-bar__cancel-btn {
  margin-left: 20rpx;
  line-height: 56rpx;
  color: #09BB07;
  white-space: nowrap;
}

